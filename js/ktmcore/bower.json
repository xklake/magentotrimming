{
  "name": "ktmcore",
  "version": "1.0.1",
  "authors": [
    "minhluan259 <minhluan259@gmail.com>"
  ],
  "description": "lib js ktmcore",
  "moduleType": [
    "globals"
  ],
  "keywords": [
    "ktmcore",
    "ktm"
  ],
  "license": "MIT",
  "homepage": "ktmcore.com",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "lib/",
    "test",
    "tests"
  ],
  "dependencies": {
    "URIjs": "~1.15.1",
    "animation.css": "~3.3.0",
    "bootstrap": "~3.3.5",
    "bootstrap-rtl": "~3.3.2",
    "bootstrap-select": "~1.7.2",
    "elegant-icons": "*",
    "fontawesome": "~4.3.0",
    "html5shiv": "~3.7.2",
    "instafeed.js": "~1.3.2",
    "jcountdown": "~2.0.0",
    "jquery": "1.11",
    "jquery-colorbox": "~1.6.1",
    "jquery-cookie": "~1.4.1",
    "jquery-lazy": "~0.5.3",
    "jquery-migrate": "~1.2.1",
    "jquery-sticky": "~1.0.1",
    "nprogress": "~0.2.0",
    "owl-carousel2": "~2.0.0-beta.3",
    "respond": "~1.4.2",
    "simple-line-icons": "~2.2.1"
  },
  "preen": {
    "jquery": [
      "dist/*.js"
    ],
    "bootstrap": [
      "dist/**/*.*"
    ],
    "bootstrap-rtl": [
      "dist/css/*.*"
    ],
    "bootstrap-select": [
      "dist/**/*.*"
    ],
    "nprogress": [
      "nprogress.css",
      "nprogress.js"
    ],
    "jquery-colorbox": [
      "*.js",
      "example1/*.*",
      "example1/**/*.*"
    ],
    "URIjs": [
      "src/*.js"
    ],
    "jquery-migrate": [
      "*.js"
    ],
    "html5shiv": [
      "dist/*.js"
    ],
    "respond": [
      "dest/*.js"
    ],
    "fontawesome": [
      "css/*.*",
      "fonts/*.*"
    ],
    "jquery-cookie": [
      "*.js"
    ],
    "jquery-sticky": [
      "*.js"
    ],
    "jquery-lazy": [
      "*.js"
    ],
    "owl-carousel2": [
      "dist/*.js",
      "dist/*/*.*"
    ],
    "simple-line-icons": [
      "css/*.*",
      "fonts/*.*"
    ],
    "animation.css": [
      "*.css"
    ],
    "jcountdown": [
      "dist/*.js"
    ],
    "elegant-icons": [
      "css/*.*",
      "fonts/*.*"
    ]
  }
}
