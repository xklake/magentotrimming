<config>
    <acl>
        <resources>
            <all>
                <title>Allow Everything</title>
            </all>
            <admin>
                <children>
                    <system>
                        <children>
                            <config>
                                <children>
                                    <less translate="title" module="less">
                                        <title>Less</title>
                                    </less>
                                </children>
                            </config>
                            <less translate="title" module="less">
                                <title>Less Files</title>
                                <sort_order>2000</sort_order>
                            </less>
                        </children>
                    </system>
                </children>
            </admin>
        </resources>
    </acl>
    <layout>
        <updates>
            <less>
                <file>ktmcore_less.xml</file>
            </less>
        </updates>
    </layout>
    <translate>
        <modules>
            <KtmCore_Less>
                <files>
                    <default>KtmCore_Less.csv</default>
                </files>
            </KtmCore_Less>
        </modules>
    </translate>
</config>